<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
              xmlns:vm="clr-namespace:DotNetTool.ViewModels"
			 x:CompileBindings="True"
			 x:DataType="vm:TextCompareViewModel"
			 x:Class="DotNetTool.TextCompareView">
	<Design.DataContext>
		<vm:TextCompareViewModel/>
	</Design.DataContext>
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="*"/>
			<ColumnDefinition Width="*"/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="*"/>
			<RowDefinition Height="Auto"/>
			<RowDefinition Height="*"/>
		</Grid.RowDefinitions>
		<StackPanel Grid.Column="0" Grid.Row="1" Grid.ColumnSpan="2"></StackPanel>
		<StackPanel Grid.Column="0" Grid.Row="2" Grid.ColumnSpan="2"></StackPanel>
	</Grid>
</UserControl>
